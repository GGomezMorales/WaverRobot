<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro" name="inertial_wheel">

    <xacro:include filename="$(find waver_description)/urdf/util.xacro"/>

    <xacro:macro name="inertial_wheel">
        <inertial>
            <mass value="${wheel_mass}" />
            <origin xyz="${triple_zero}" rpy="${triple_zero}" />
            <inertia ixx="${I_wheel}" ixy="0" ixz="0" iyy="${I_wheel}" iyz="0" izz="${Iz_wheel}" />
        </inertial>
    </xacro:macro>

</robot>
